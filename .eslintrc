{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/sort-comp": 0,
    "comma-dangle": [
      2,
      "never"
    ],
    "no-shadow": 0,
    "brace-style": [
      2,
      "stroustrup"
    ]
  },
  "plugins": [
    "react"
  ]
}